<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dwdemo"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="19" />

    <application
        android:name="com.example.dwdemo.DWDemo"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.dwdemo.MainActivity"
            android:label="@string/app_name"
            android:launchMode="singleTask" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            <intent-filter>
                <action android:name="com.motorolasolutions.emdk.sample.dwdemosample.RECVR" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.dwdemo.CamActivity"
            android:label="cam activity"
            android:screenOrientation="landscape" >
        </activity>
        <activity
            android:name="com.example.dwdemo.CameraAct"
            android:label="@string/title_activity_camera" >
        </activity>
        <activity
            android:name="com.example.dwdemo.DWActivity"
            android:label="@string/title_activity_camera" >
        </activity>


    </application>

    <uses-permission android:name="android.permission.CAMERA" />

</manifest>